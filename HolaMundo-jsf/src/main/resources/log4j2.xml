<?xml version="1.0" encoding="UTF-8"?>

<Configuration status="info">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <patternLayout pattern="%d{HH:mm:ss} [%t] %-5level %logger{36} -%msg%n"></patternLayout>
        </Console>
    </Appenders>
    <loggers>
        <Root level="info">
            <AppenderRef ref="Console"/>
        </Root>
    </loggers>
</Configuration>